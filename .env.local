# JARVIS Frontend Configuration

# Backend API URL (Hugging Face Spaces)
NEXT_PUBLIC_API_URL=https://YOUR-USERNAME-jarvis-api.hf.space

# WebSocket URL (same as API)
NEXT_PUBLIC_WS_URL=wss://YOUR-USERNAME-jarvis-api.hf.space

# App Configuration
NEXT_PUBLIC_APP_NAME=JARVIS
NEXT_PUBLIC_APP_VERSION=2.0.0

# Features (enable/disable)
NEXT_PUBLIC_ENABLE_VOICE=true
NEXT_PUBLIC_ENABLE_VISION=true
NEXT_PUBLIC_ENABLE_3D=true

# Analytics (optional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX